{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            ["styled-jsx-plugin-sass"],
            [
              "styled-jsx-plugin-postcss",
              {
                "path": "./styles/postcss.config.js"
              }
            ]
          ]
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@common": ["./common"],
          "@interfaces": ["./interfaces"],
          "@enviroment": ["./enviroment"],
          "@components": ["./components"],
          "@services": ["./services"],
          "@styles": ["./styles"],
          "@redux": ["./redux"],
          "@svgs": ["./svgs"]
        }
      }
    ]
  ]
}
